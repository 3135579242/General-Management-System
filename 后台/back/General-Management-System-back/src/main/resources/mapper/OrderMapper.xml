<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.along.generalmanagementsystemback.mapper.OrderMapper">


    <resultMap id="orderMap" type="com.along.generalmanagementsystemback.domain.entity.OrderTotal" >
        <id property="id" column="id"/>
